<h3>Users</h3>
<div class="span8">
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Email</th>
        <th>Registered on</th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= link_to user.email, user %>
          </td>
          <td>
            <%= user.created_at.to_date %>
          </td>
          <td>
            <%= render user %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>